<!DOCTYPE html>
<html>
<head>
    <meta name="layout" content="ebay"/>
    <title>Contact</title>
</head>

<body>
<div class="row">
    <!-- CONTACT CONTENT -->
    <section class="content-wrapper">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <div class="content-box">
                        <h3>Maps</h3>

                        <div class="map" id="map" data-lat='48.8620722' data-lng='2.352047'
                             data-address="<strong>Serenity Decoration</strong><br/><br/>Victoria Mole St. 15 <br/> NY, United States<br/><br/>"
                             data-color="#26292E">
                        </div>
                    </div>
                </div>

                <div class="col-md-6">
                    <div class="content-box">
                        <h3>Contact</h3>

                        <p>
                            <strong>Housing Store</strong>
                            <br/>901 Cherry Ave, San Bruno, CA 94066, United States
                        </p>

                        <form role="form">
                            <div class="form-group">
                                <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Email">
                            </div>

                            <div class="form-group">
                                <input type="text" class="form-control" id="exampleInputPassword1"
                                       placeholder="Website">
                            </div>

                            <div class="form-group">
                                <textarea class="form-control" rows="5"></textarea>
                            </div>

                            <div class="checkbox">
                                <label>
                                    <input type="checkbox">Check me out
                                </label>
                            </div>
                            <button type="submit" class="btn btn-default">Send Email</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- END CONTACT CONTENT -->
</div>
</body>
</html>